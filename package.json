{
  "name": "opstooling-js",
  "description": "opstooling-js",
  "version": "0.0.2",
  "author": "Parity <admin@parity.io> (https://parity.io)",
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --loglevel silent '{*,**/*}.{json,html}'",
    "fix:eslint": "eslint --fix",
    "fix:prettier": "prettier --write",
    "fix": "yarn fix:eslint '{*,**/*}.{cjs,ts}' && yarn fix:prettier '{*,**/*}.json'"
  },
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "require": "./build/index.js",
      "types": "./build/index.d.ts"
    }
  },
  "devDependencies": {
    "@resolritter/tsc-files": "^1.1.4",
    "@types/node": "^17.0.24",
    "fastify": "^3.29.0",
    "opstooling-js-style": "https://github.com/paritytech/opstooling-js-style#6cfdf3caaacd483d330b9ab534625fe33adb26b5",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "@octokit/rest": "^18.12.0",
    "async-mutex": "^0.3.2",
    "probot": "^12.2.2"
  }
}
