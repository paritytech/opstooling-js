{
  "name": "opstooling-js",
  "description": "opstooling-js",
  "version": "0.0.0",
  "author": "Parity <admin@parity.io> (https://parity.io)",
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc && tsc-alias",
    "build_watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "fix": "yarn fix:eslint '{*,**/*}.{cjs,js,ts}' && yarn fix:prettier '{*,**/*}.json'",
    "fix:eslint": "eslint --fix",
    "fix:prettier": "prettier --write",
    "generate-types": "node ./dist/cli/main.js generate-types --schemas dist/schemas --out src/types/generated",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "bin": {
    "opstooling-scripts": "./dist/cli/main.js"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@resolritter/tsc-files": "^1.1.4",
    "@types/jest": "^27.5.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.24",
    "@types/node-fetch": "^2.6.1",
    "concurrently": "^7.5.0",
    "fastify": "^3.29.0",
    "jest": "^28.1.0",
    "opstooling-js-style": "https://github.com/paritytech/opstooling-js-style#c5003bcb6febd8872179c278b08704137ab9b509",
    "ts-jest": "^28.0.2",
    "tsc-alias": "^1.7.1",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "@octokit/rest": "^18.12.0",
    "async-mutex": "^0.3.2",
    "commander": "^9.3.0",
    "deepdash": "^5.3.9",
    "joi": "^17.6.4",
    "joi-to-typescript": "^4.0.5",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "prettier": "^2.7.1",
    "probot": "^12.2.8"
  }
}
